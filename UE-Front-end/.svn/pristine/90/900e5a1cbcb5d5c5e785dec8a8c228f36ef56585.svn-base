.head {
    position: fixed;
    top: 0;
    width: 100vw;
    height: 4vh;
    z-index: 50;
    background: linear-gradient(180deg, rgba(0,1,34,0.8) 0%, #006D6F 100%);
    display: flex;
    flex-direction: row;
    align-items: center;
}

.logo {
    height: 3vh;
    width: 7vw;
    margin-left: 0.4vw;
}

.cut {
    position: absolute;
    z-index: -1;
    left: 8vw;
    top: 0.4vh;
    width: 91.7vw;
    height: 3.3vh;
}

.clock {
    margin-left: 1.1vw;
    width: 5vw;
    height: auto;
}

.time {
    position: relative;
    margin-left: 0.5vw;
    margin-top: 1.4vh;
    /* width: 16.8vw;
    height: 2.2vh; */
}

.timeline {
    width: 16.8vw;
    height: 2.5vh;
}

.circle1 {
    position: absolute;
    width: 3%;
    top: 0.1vh;
}

.func {
    display: flex;
    margin-left: auto;
    margin-right: 0.3vw;
}

.funcbox {
    width: 1.8vw;
    height: 3.1vh;
    display: flex;
    justify-content: center;
    align-items: center;
}

.funcbox:hover {
    background: linear-gradient(180deg, rgba(255,255,255,0.8) 0%, #13769F 100%);
    border-radius: 0.2vw;
}